<span class="nb">Map</span><span class="p">.</span><span class="nx">addLayer</span><span class="p">(</span><span class="nx">annualGreenest</span><span class="p">.</span><span class="nx">clip</span><span class="p">(</span><span class="nx">setExtent</span><span class="p">).</span><span class="nx">select</span><span class="p">(</span><span class="dl">'</span><span class="s1">GI_max_14</span><span class="dl">'</span><span class="p">),</span>
